{{if not .IsReadOnly}}
	w.starlark{{.GoName}} = {{.StarlarkName}}
	{
{{if eq .GoName "X"}}
		if val, err := NumberOrPercentageFromStarlark({{.StarlarkName}}, -math.MaxFloat64, math.MaxFloat64, map[string]float64{"left": 0.0, "center": 0.5, "right": 1.0}); err == nil {
{{else if eq .GoName "Y"}}
		if val, err := NumberOrPercentageFromStarlark({{.StarlarkName}}, -math.MaxFloat64, math.MaxFloat64, map[string]float64{"top": 0.0, "center": 0.5, "bottom": 1.0}); err == nil {
{{else}}
		if val, err := NumberOrPercentageFromStarlark({{.StarlarkName}}, -math.MaxFloat64, math.MaxFloat64, map[string]float64{}); err == nil {
{{end}}
			w.{{.GoName}} = val
		} else {
			return nil, err
		}
	}
{{end}}
